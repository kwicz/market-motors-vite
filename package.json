{
  "name": "market-motors-vite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "nodemon --exec \"tsx server/index.ts\"",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "tsc && vite build",
    "build:dev": "vite build --mode development",
    "build:server": "tsc --project server/tsconfig.json",
    "start": "node server/dist/index.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start:server": "node server/dist/index.js",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:server": "vitest run --config vitest.server.config.ts",
    "test:integration": "vitest run --config vitest.server.config.ts server/__tests__/integration/",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:db": "vitest run --config vitest.server.config.ts lib/db/__tests__/",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "db:backup": "tsx scripts/db-manager.ts backup",
    "db:backup:list": "tsx scripts/db-manager.ts backup:list",
    "db:backup:restore": "tsx scripts/db-manager.ts backup:restore",
    "db:backup:verify": "tsx scripts/db-manager.ts backup:verify",
    "db:status": "tsx scripts/db-manager.ts db:status",
    "db:seed": "tsx scripts/db-manager.ts db:seed",
    "db:reset": "tsx scripts/db-manager.ts db:reset",
    "db:maintenance": "tsx scripts/db-manager.ts maintenance",
    "db:migrate:status": "tsx scripts/db-manager.ts migrate:status",
    "db:migrate:generate": "tsx scripts/db-manager.ts migrate:generate",
    "db:migrate:rollback": "tsx scripts/db-manager.ts migrate:rollback",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:test": "tsx lib/db/test-connection.ts",
    "deploy:railway": "./scripts/deploy-railway.sh",
    "deploy:build": "npm run build && npm run db:migrate",
    "postinstall": "echo 'Frontend build ready'"
  },
